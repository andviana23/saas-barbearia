{
  "name": "saas-barbearia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "db:migrate": "node db/migrate-fixed.js",
    "db:status": "node db/migrate-fixed.js --status",
    "db:help": "node db/migrate-fixed.js --help",
    "db:prepare": "node db/prepare-sql.js",
    "db:verify": "node db/verify.js",
    "db:seed": "node db/run-seeds.js",
    "db:test:reset": "NODE_ENV=test npm run db:migrate",
    "test": "jest",
    "test:unit": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coverage": "jest --coverage",
    "coverage:actions:debug": "jest src/actions/__tests__/agendamentos.test.ts --coverage --silent",
    "coverage:zero:list": "node scripts/coverage/list-zero-coverage.js coverage/coverage-summary.json || echo 'Gerar summary primeiro com test:coverage'",
    "test:coverage:ci": "cross-env JEST_JUNIT_OUTPUT=./test-results/junit.xml jest --coverage --config=jest.config.ci.js && npm run coverage:verify",
    "coverage:verify": "node scripts/verify-coverage.js",
    "coverage:thresholds:print": "node scripts/jest/print-thresholds.js",
    "jest:config:dump": "node scripts/jest/dump-final-config.js",
    "rls:generate": "node db/rls-introspect.js && node scripts/jest/generate-rls-matrix.js",
    "rls:generate:offline": "node scripts/jest/generate-rls-matrix-offline.js",
    "rls:expected:sync": "npm run rls:generate && node scripts/jest/rls-expected-sync.js",
    "rls:expected:sync:offline": "npm run rls:generate:offline && node scripts/jest/rls-expected-sync.js",
    "rls:expected:auto": "node scripts/jest/rls-expected-auto-classify.js",
    "rls:crud:test": "npm run rls:expected:sync && jest tests/rls.crud.test.ts --runInBand",
    "rls:expected:check": "cross-env RLS_EXPECTED_STRICT=1 jest tests/rls.expected.test.ts --runInBand",
    "rls:expected:summary": "node scripts/jest/rls-expected-summary.js",
    "rls:report": "node scripts/jest/rls-report.js",
    "rls:exec:merge": "node scripts/jest/rls-exec-merge.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:debug": "playwright test --debug",
    "e2e": "node scripts/run-e2e.js",
    "e2e:local": "node scripts/run-e2e.js -e local -s smoke",
    "e2e:staging": "node scripts/run-e2e.js -e staging -s critical",
    "e2e:production": "node scripts/run-e2e.js -e production -s smoke",
    "e2e:critical": "node scripts/run-e2e.js -s critical",
    "e2e:full": "node scripts/run-e2e.js -s full",
    "e2e:performance": "node scripts/run-e2e.js -s performance",
    "e2e:mobile": "node scripts/run-e2e.js -s mobile",
    "e2e:debug": "node scripts/run-e2e.js --debug",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "backup": "bash scripts/backup/daily-logical-backup.sh",
    "recovery": "bash scripts/recovery/disaster-recovery.sh",
    "docker:build": "docker build -t saas-barbearia .",
    "docker:run": "docker run -p 3000:3000 saas-barbearia",
    "docker:prod": "docker-compose -f docker-compose.production.yml up -d",
    "ci": "npm run lint && npm run type-check && npm run test:unit && npm run rls:expected:check",
    "ci:full": "npm run ci && npm run test:e2e"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@mui/material-nextjs": "^6.5.0",
    "@mui/x-charts": "^8.10.2",
    "@mui/x-data-grid": "^8.10.2",
    "@mui/x-date-pickers": "^8.10.2",
    "@mui/x-date-pickers-pro": "^8.10.2",
    "@mui/x-tree-view": "^8.10.2",
    "@sentry/nextjs": "^10.5.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "next": "14.2.5",
    "openai": "^5.15.0",
    "pg": "^8.16.3",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@modelcontextprotocol/server-postgres": "^0.6.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.1.5",
    "msw": "^2.2.1",
    "prettier": "^3.6.2",
    "supabase": "^2.34.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
